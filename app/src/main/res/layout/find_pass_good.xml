<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/some_id"
    android:layout_width="910px"
    android:layout_height="680px"
    android:clipToOutline="true"
    android:background="@drawable/member"
    >

    <RelativeLayout
        android:id="@+id/change_password"
        android:layout_width="910px"
        android:layout_height="680px"
        android:background="@drawable/membership"
        android:clipToOutline="true">


        <TextView
            android:id="@+id/text_find_hak_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="133dp"
            android:layout_marginTop="11dp"
            android:gravity="center_horizontal|center_vertical"
            android:text="비밀번호 찾기"
            android:textAppearance="@style/some_id" />

        <View
            android:id="@+id/line_1_p"
            android:layout_width="800px"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/line_1" />

        <TextView
            android:id="@+id/find_id_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="80px"
            android:layout_marginTop="140px"
            android:gravity="center_vertical"
            android:text="새로운 비밀번호"
            android:textAppearance="@style/some_id" />

        <EditText
            android:id="@+id/ch_pass"
            android:layout_width="689.34px"
            android:layout_height="85.84px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="80px"
            android:layout_marginTop="215px"
            android:background="@drawable/edit_id"
            android:elevation="10dp"
            android:hint="비밀번호를 입력하세요."
            android:paddingLeft="27px"
            android:textSize="36px" />

        <TextView
            android:id="@+id/find_pass_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="80px"
            android:layout_marginTop="325px"
            android:gravity="center_vertical"
            android:text="비밀번호 확인"
            android:textAppearance="@style/some_id" />

        <EditText
            android:id="@+id/ch_pass_check"
            android:layout_width="689.34px"
            android:layout_height="85.84px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="80px"
            android:layout_marginTop="390px"
            android:background="@drawable/edit_id"
            android:elevation="10dp"
            android:hint="같은 비밀번호를 입력하세요."
            android:paddingLeft="27px"
            android:textSize="36px" />

        <android.widget.Button
            android:id="@+id/submit_btn_c"
            android:layout_width="455px"
            android:layout_height="105.87px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="455px"
            android:layout_marginTop="580px"
            android:background="#FF002057"
            android:text="확인"
            android:textColor="#ffffff" />

        <android.widget.Button
            android:id="@+id/cancel_btn_c"
            android:layout_width="455px"
            android:layout_height="105.87px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="0px"
            android:layout_marginTop="580px"
            android:background="#ffffff"
            android:text="취소"
            android:textColor="#000000" />

    </RelativeLayout>
</RelativeLayout>